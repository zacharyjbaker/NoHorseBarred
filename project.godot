config_version=5

[application]

config/name="ThroughTheHaze"
run/main_scene="res://Scenes/node_2d.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[aseprite]

import/import_plugin/default_automatic_importer="SpriteFrames"

[aseprite_importers]

aseprite_executable_path="\"/Library/Application Support/Steam/steamapps/common/Aseprite\""

[autoload]

Global="*res://Scripts/global.gd"

[display]

window/size/mode=3
window/size/borderless=true
window/stretch/aspect="keep_width"
window/stretch/scale=2.5
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="Cephalopioneers"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/nklbdev.aseprite_importers/plugin.cfg")

[importer_defaults]

"Aseprite AnimatedSprite2D Import"={
"animation/autoplay": "",
"animation/default/direction": 0,
"animation/default/name": "default",
"animation/default/repeat_count": 0,
"spritesheet/border_type": 0,
